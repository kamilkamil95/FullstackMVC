@model IEnumerable<ApplicationTypeModel>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">app  List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-controller="ApplicationType" asp-action="Create" class="btn btn-primary"> Create new Application</a>
        </div>

    </div>
    <br /><br />


    @if (Model.Count() > 0)
    {
        <table class=" = table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>
                        ApplicationType
                    </th>
                    <th>
                        Display Order
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td width="50%">@item.Id</td>
                    <td width="50%">@item.Name</td>
                </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>No category exist</p>
    }

</div>